{
  "name": "redux-boilerplate",
  "private": true,
  "description": "React / Redux / Redux-elm Boilerplate",
  "author": "Salsita Software (https://www.salsitasoft.com/)",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:salsita/redux-boilerplate.git"
  },
  "homepage": "https://www.salsitasoft.com/",
  "scripts": {
    "postinstall": "npm run create_local_config && npm run lint && npm test && npm run build:be && npm run build:fe",
    "build:fe": "./node_modules/.bin/webpack --config=webpack/webpack.prod.frontend.config.babel.js",
    "build:be": "./node_modules/.bin/webpack --config=webpack/webpack.prod.backend.config.babel.js",
    "start": "node ./dist/server.js",
    "start:dev": "export NODE_ENV=development && export API_BASE=http://localhost:3000 && export PORT=3001 && ./node_modules/.bin/babel-node devRunner.js",
    "start:native": "export API_BASE=http://localhost:3000 && node node_modules/react-native/local-cli/cli.js start",
    "lint": "./node_modules/.bin/eslint ./src",
    "test": "./node_modules/.bin/mocha --require babel/babelhook --recursive",
    "test:watch": "npm test -- --watch",
    "create_local_config": "touch src/server/config/local.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-native": "^1.8.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.0",
    "css-loader": "^0.23.0",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.0.1",
    "estraverse": "^4.1.1",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.3.4",
    "postcss-loader": "^0.9.1",
    "request": "^2.65.0",
    "shebang-regex": "^2.0.0",
    "single-child": "^0.3.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.1.0",
    "webpack": "^1.12.4",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "assign-deep": "^0.4.3",
    "babel-runtime": "^6.9.0",
    "express": "^4.13.4",
    "history": "^2.1.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "15.0.2",
    "react-dom": "15.0.2",
    "react-native": "^0.26.1",
    "react-redux": "^4.1.2",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.4",
    "redux": "^3.3.1",
    "redux-elm": "^2.1.5",
    "redux-saga": "^0.10.4"
  }
}
